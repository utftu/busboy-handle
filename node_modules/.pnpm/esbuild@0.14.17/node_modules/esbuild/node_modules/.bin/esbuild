#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/a.malyuta/projects/busboy-handler/node_modules/.pnpm/esbuild@0.14.17/node_modules/esbuild/bin/node_modules:/Users/a.malyuta/projects/busboy-handler/node_modules/.pnpm/esbuild@0.14.17/node_modules/esbuild/node_modules:/Users/a.malyuta/projects/busboy-handler/node_modules/.pnpm/esbuild@0.14.17/node_modules:/Users/a.malyuta/projects/busboy-handler/node_modules/.pnpm/node_modules:/Users/a.malyuta/projects/busboy-handler/node_modules:/Users/a.malyuta/projects/node_modules:/Users/a.malyuta/node_modules:/Users/node_modules:/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/a.malyuta/projects/busboy-handler/node_modules/.pnpm/esbuild@0.14.17/node_modules/esbuild/bin/node_modules:/Users/a.malyuta/projects/busboy-handler/node_modules/.pnpm/esbuild@0.14.17/node_modules/esbuild/node_modules:/Users/a.malyuta/projects/busboy-handler/node_modules/.pnpm/esbuild@0.14.17/node_modules:/Users/a.malyuta/projects/busboy-handler/node_modules/.pnpm/node_modules:/Users/a.malyuta/projects/busboy-handler/node_modules:/Users/a.malyuta/projects/node_modules:/Users/a.malyuta/node_modules:/Users/node_modules:/node_modules"
fi
"$basedir/../../bin/esbuild"   "$@"
exit $?
