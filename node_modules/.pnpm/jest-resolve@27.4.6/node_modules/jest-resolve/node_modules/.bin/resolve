#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/a.malyuta/projects/busboy-handler/node_modules/.pnpm/resolve@1.22.0/node_modules/resolve/bin/node_modules:/Users/a.malyuta/projects/busboy-handler/node_modules/.pnpm/resolve@1.22.0/node_modules/resolve/node_modules:/Users/a.malyuta/projects/busboy-handler/node_modules/.pnpm/resolve@1.22.0/node_modules:/Users/a.malyuta/projects/busboy-handler/node_modules/.pnpm/node_modules:/Users/a.malyuta/projects/busboy-handler/node_modules:/Users/a.malyuta/projects/node_modules:/Users/a.malyuta/node_modules:/Users/node_modules:/node_modules:/Users/a.malyuta/projects/busboy-handler/node_modules/.pnpm/jest-resolve@27.4.6/node_modules/jest-resolve/node_modules:/Users/a.malyuta/projects/busboy-handler/node_modules/.pnpm/jest-resolve@27.4.6/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/a.malyuta/projects/busboy-handler/node_modules/.pnpm/resolve@1.22.0/node_modules/resolve/bin/node_modules:/Users/a.malyuta/projects/busboy-handler/node_modules/.pnpm/resolve@1.22.0/node_modules/resolve/node_modules:/Users/a.malyuta/projects/busboy-handler/node_modules/.pnpm/resolve@1.22.0/node_modules:/Users/a.malyuta/projects/busboy-handler/node_modules/.pnpm/node_modules:/Users/a.malyuta/projects/busboy-handler/node_modules:/Users/a.malyuta/projects/node_modules:/Users/a.malyuta/node_modules:/Users/node_modules:/node_modules:/Users/a.malyuta/projects/busboy-handler/node_modules/.pnpm/jest-resolve@27.4.6/node_modules/jest-resolve/node_modules:/Users/a.malyuta/projects/busboy-handler/node_modules/.pnpm/jest-resolve@27.4.6/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../resolve@1.22.0/node_modules/resolve/bin/resolve" "$@"
else
  exec node  "$basedir/../../../../../resolve@1.22.0/node_modules/resolve/bin/resolve" "$@"
fi
